{
  "name": "create-react-app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "wood </>",
  "license": "MIT",
  "scripts": {
    "debug": "node --inspect-brk ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode=development --config=./web.webpack.config.js",
    "start": "webpack-dev-server --mode=development --config=./web.webpack.config.js",
    "build:web": "webpack --config=./web.webpack.config.js",
    "build:web:dev": "yarn build:web --mode=development",
    "build:web:pro": "yarn build:web --mode=production",
    "build:node": "webpack --config=./node.webpack.config.js --output-public-path=/statics/",
    "build:node:dev": "yarn build:node --mode=development",
    "build:node:pro": "yarn build:node --mode=production",
    "build:all": "yarn build:web --output-public-path=/statics/ && yarn build:node",
    "build:all:pro": "yarn build:web:pro --output-public-path=/statics/ && yarn build:node:pro",
    "eslint": "eslint -c ./.eslintrc.js --ext .js --ext .jsx --no-ignore '.eslintrc.js' .eslintrc.js .",
    "pm2": "pm2 kill && pm2 start && pm2 monit"
  },
  "dependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "abab": "^2.0.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "localforage": "^1.7.3",
    "lodash-es": "^4.17.15",
    "luxon": "^1.21.3",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.0",
    "pm2": "^4.2.1",
    "polka": "^0.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "selfsigned": "^1.10.7",
    "serve-static": "^1.14.1",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-core": "^4.3.1",
    "workbox-precaching": "^4.3.1",
    "workbox-webpack-plugin": "^4.3.1",
    "worker-loader": "^2.0.0"
  }
}
